	<queryString>
		<![CDATA[SELECT d.cod_pa, d.nombres_pa||' '||d.apellidos_pa as nombre,obtener_edad(d.fecha_nacimiento_pa,e.fecha_examen)as edad, n.n_orden, e.fecha_examen,  e.chkigm_reactivo, e.chkigm_noreactivo,
       e.chkigg_reactivo, e.chkigg_noreactivo,e.chkinvalido,n.color
                FROM datos_paciente AS d
                INNER JOIN n_orden_ocupacional AS n ON (d.cod_pa = n.cod_pa)
	      INNER JOIN examen_inmunologico AS e ON (n.n_orden = e.n_orden)
               WHERE e.n_orden =$P{Norden}]]>
	</queryString>
