	<queryString>
		<![CDATA[SELECT d.cod_pa, d.nombres_pa||' '||d.apellidos_pa as nombre, d.fecha_nacimiento_pa ,d.sexo_pa, n.razon_empresa,n.razon_contrata,n.cargo_de ,c.*,  obtener_edad(d.fecha_nacimiento_pa,c.fecha)as edad, n.color, n.cod_sede
	FROM datos_paciente AS d
          INNER JOIN n_orden_ocupacional AS n ON (d.cod_pa = n.cod_pa)
	inner JOIN consentimientobuenasalud AS c ON(c.n_orden = n.n_orden)
          WHERE c.n_orden =$P{Norden}]]>
	</queryString>
