	<queryString>
		<![CDATA[SELECT distinct (d.cod_pa),  d.nombres_pa||' ' ||d.apellidos_pa as nombres
FROM ticket AS t
INNER JOIN datos_paciente_asistencial AS d ON (d.cod_pa = t.cod_pa)
INNER JOIN ticket_contenido AS tc ON (tc.n_ticket= t.n_ticket)

WHERE
    t.f_ticket >= $P{Fecha1}
AND
    t.f_ticket <= $P{Fecha2}]]>
	</queryString>
