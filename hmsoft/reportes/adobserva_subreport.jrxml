	<parameter name="Norden" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT n.n_orden, o.fecha_obser, o.hora_obser, o.examenes
FROM datos_paciente as dp
inner join n_orden_ocupacional AS n ON(dp.cod_pa = n.cod_pa)
inner join observaciones as o on(n.n_orden=o.n_orden)
where n.cod_pa=$P{Norden}
order by n.n_orden desc limit 3]]>
	</queryString>
