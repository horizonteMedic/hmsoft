	<parameter name="Norden" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT tc.n_ticket, tc.p_cantidad, tc.p_descripcion,
       tc.p_unitario::numeric, tc.p_total::numeric,t.descuento::numeric
  FROM ticket as t
  inner join datos_paciente_asistencial as d on(t.cod_pa=d.cod_pa)
  inner join ticket_contenido as tc on(t.n_ticket=tc.n_ticket)
  where tc.n_ticket= $P{Norden}
]]>
	</queryString>
