	<parameter name="Fecha1" class="java.util.Date"/>
	<parameter name="Fecha2" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT to_char(t.n_ticket,'0000000')AS numero, t.nom_medico, t.total::numeric,t.descuento::numeric, t.f_ticket, t.h_ticket, d.nombres_pa||' ' ||d.apellidos_pa as nombres
FROM ticket AS t INNER JOIN datos_paciente_asistencial AS d ON (d.cod_pa = t.cod_pa)
    WHERE
    t.f_ticket >= $P{Fecha1}
AND
    t.f_ticket <= $P{Fecha2}]]>
	</queryString>
